<tr>
  <td><%= I18n.t('spree.log_entry.stripe.message') %></td>
  <td><%= entry.parsed_details.message %></td>
</tr>
<tr>
  <td><%= I18n.t('spree.log_entry.stripe.charge_id') %></td>
  <td><%= entry.parsed_details.params['id'] %></td>
</tr>
<% if card = entry.parsed_details.params['card'] %>
  <tr>
    <td><%= I18n.t('spree.log_entry.stripe.card_id') %></td>
    <td><%= card['id'] %></td>
  </tr>

  <tr>
    <td><%= I18n.t('spree.log_entry.stripe.cvc_check') %></td>
    <td><%= card['cvc_check'] %></td>
  </tr>

  <tr>
    <td><%= I18n.t('spree.log_entry.stripe.address_zip_check') %></td>
    <td><%= card['address_zip_check'] %></td>
  </tr>
<% end %>
<tr>
  <td><%= I18n.t('spree.log_entry.stripe.cvv_result') %></td>
  <td><%= entry.parsed_details.cvv_result['message'].to_s %></td>
</tr>
